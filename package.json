{
  "name": "notes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "react": "^16.8.3",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.8.3",
    "react-scripts": "2.1.5"
  },
  "scripts": {
    "start": "npm run build:sass && react-scripts start && npm run watch:sass",
    "build": "npm run build:sass && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:sass": "npm run build:sass && npm run build:sass -- -w",
    "build:sass": "node-sass -r --output-style compressed src/stylesheets/main.scss -o src/stylesheets/",
    "lint": "eslint src --ext .jsx --ext .js",
    "lint-fix": "eslint --fix src --ext .jsx --ext .js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "node-sass": "^4.11.0"
  }
}
